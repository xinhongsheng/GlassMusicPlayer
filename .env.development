# 本地环境
VITE_USER_NODE_ENV = development

# 公共基础路径
VITE_PUBLIC_PATH = /

# 路由模式
# Optional: hash | history
VITE_ROUTER_MODE = history

# 开发环境接口地址（走本地代理避免 CORS）
VITE_APP_BASE_API = '/api'
# 开发环境跨域代理，支持配置多个
VITE_PROXY = [["/api","http://localhost:3000"]]

# 开发环境媒体代理（解决图片/音频 CORS）
VITE_MEDIA_PROXY = '/media?url='
